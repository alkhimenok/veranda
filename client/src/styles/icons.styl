@font-face
  font-family 'icons'
  src url('./assets/icons/icons.eot?ombvb')
  src url('./assets/icons/icons.eot?ombvb#iefix') format('embedded-opentype'), url('./assets/icons/icons.ttf?ombvb') format('truetype'), url('./assets/icons/icons.woff?ombvb') format('woff'), url('./assets/icons/icons.svg?ombvb#icons') format('svg')
  font-weight normal
  font-style normal
  font-display block

[class^='_icon-'],
[class*=' _icon-']
  font-family 'icons' !important
  speak never
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

